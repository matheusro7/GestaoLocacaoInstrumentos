@model GestaoLocacaoInstrumentos.Models.Funcionario

<h2>Editar Funcionário</h2>

<form asp-action="Edit">
    <div class="form-group">
        <label for="Nome">Nome</label>
        <input asp-for="Nome" class="form-control" />
    </div>
    <div class="form-group">
        <label for="Cargo">Cargo</label>
        <input asp-for="Cargo" class="form-control" />
    </div>
    <div class="form-group">
        <label for="Senha">Senha</label>
        <input asp-for="Senha" type="password" class="form-control" />
    </div>
    <div class="form-group">
        <label for="SearchString">SearchString</label>
        <input asp-for="SearchString" class="form-control" />
    </div>
    <button type="submit" class="btn btn-primary">Salvar</button>
</form>
