@model GestaoLocacaoInstrumentos.Models.Agendamento

<h4>Tem certeza que deseja excluir o seguinte Agendamento?</h4>

<div>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            Estúdio:
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Estudio)
        </dd>
        <dt class="col-sm-2">
            Data:
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Data)
        </dd>
        <dt class="col-sm-2">
            Hora Início:
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.HoraInicio)
        </dd>
        <dt class="col-sm-2">
            Hora Fim:
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.HoraFim)
        </dd>
        <dt class="col-sm-2">
            Cliente:
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Cliente)
        </dd>
        <dt class="col-sm-2">
            Valor:
        </dt>
        <dd class="col-sm-8">@Model.Valor.ToString("C")</dd>
        <dd class="col-sm-10">
        </dd>
    </dl>
</div>

<form asp-action="Delete" method="post">
    <input type="hidden" asp-for="Id" />
    <button type="submit" class="btn btn-danger">Confirmar Exclusão</button>
    <a asp-action="Index" class="btn btn-secondary">Cancelar</a>
</form>



